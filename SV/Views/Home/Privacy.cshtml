@{
    ViewData["Title"] = "Consulta de Propietarios de un Predio";
}


<div class="container">
    <h1>@ViewData["Title"]</h1>
    <form asp-action="Privacy" asp-controller="Home" method="get">
        <div class="form-group row">
            <div class="col-sm-10">
                <input type="text" class="form-control" id="comuna" name="comuna" value="@ViewData["CurrentCommune"]" placeholder="Comuna">
            </div>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="manzana" name="manzana" value="@ViewData["CurrentBlock"]"  placeholder="Manzana">
            </div>
            <div class="col-sm-10">
                <input type="text" class="form-control" id="predio" name="predio" value="@ViewData["CurrentProperty"]"  placeholder="Predio">
            </div>
        </div>
        <div class="form-group">
            <label for="year">Año:</label>
            <select class="form-control" id="year" name="year"></select>
        </div>
        <button type="submit" class="btn btn-primary">Enviar</button>
    </form>
</div>


<script src="~/js/multiowner.js" asp-append-version="true"></script>
