@model SV.Models.RealStateForm

@{
    ViewData["Title"] = "Detalles";
}

<h1>@ViewData["Title"]</h1>

<div>
    <h4>Formulario de Bienes Raíces  |  N° de Atención: @Html.DisplayFor(model => model.AttentionNumber)</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            CNE
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NatureOfTheDeed)
        </dd>
        <dt class = "col-sm-2">
           Comuna
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Commune)
        </dd>
        <dt class = "col-sm-2">
            Manzana
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Block)
        </dd>
        <dt class = "col-sm-2">
            Predio
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Property)
        </dd>
        <dt class = "col-sm-2">
            Fojas
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Sheets)
        </dd>
        <dt class = "col-sm-2">
            Fecha Inscripción
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InscriptionDate)
        </dd>
        <dt class = "col-sm-2">
            Número Inscripción
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.InscriptionNumber)
        </dd>
    </dl>
</div>
<div>
    <h5>Enajenante(s) o Causante(s)</h5>
    <table class="table">
        <thead>
            <tr>
                <th>
                    RUT
                </th>
                <th>
                    % de Derecho
                </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                    algo
                </td>
                <td>
                    algo
                </td>
            </tr>
        </tbody>
    </table>
</div>
<div>
    <h5>Adquiriente(s) o Heredero(s)</h5>
    <table class="table">
        <thead>
            <tr>
                <th>
                    RUT
                </th>
                <th>
                    % de Derecho
                </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>
                    algo
                </td>
                <td>
                    algo
                </td>
            </tr>
        </tbody>
    </table>
</div>
